<h1>I'm the edit shopping list page</h1>


 <form action="/edit-list" method="POST"> 
  <fieldset>
    <label for="shoppingListName">Shopping List Name:</label>
    <input type="text" id="shoppingListName" name="shoppingListName">
    
    <br>
    <br>
    <br>

    {{#each Lists.nameList}}
      <p>Shopping List Name: {{this.nameList}}</p>
        <ul>Items
          {{#each this.itemNanme}}
            <li>{{this}}</li>
          {{/each}}
        </ul>
    {{/each}}

    <label for="addItem">+ Item</label>
    <input type="text" id="addItem" name="addItem">

    <button type="button" class="btn btn-success">Add Item</button>

  </fieldset>
  

</form>